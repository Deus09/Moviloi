{
  "name": "moviloi",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/check-env.cjs",
    "build": "npm run prebuild && vite build",
    "build:analyze": "npm run build && npm run analyze:size",
    "serve": "vite preview",
    "tailwind:build": "postcss src/index.css -o src/tailwind.output.css",
    "analyze:size": "node scripts/analyze-bundle.cjs",
    "analyze:open": "open dist/bundle-analyzer.html || xdg-open dist/bundle-analyzer.html || start dist/bundle-analyzer.html",
    "size:check": "size-limit",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "pretest:e2e": "playwright install chromium webkit",
    "test:e2e:deps": "playwright install-deps",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "PWDEBUG=1 playwright test",
    "test:e2e:trace": "playwright test --trace on",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "test:all": "npm run test:unit && npm run test:e2e",
    "ci:test": "npm run lint && npm run test:unit && npm run test:e2e",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/app": "7.0.1",
    "@capacitor/core": "7.4.2",
    "@capacitor/haptics": "7.0.1",
    "@capacitor/ios": "^7.4.2",
    "@capacitor/keyboard": "7.0.1",
    "@capacitor/network": "^7.0.2",
    "@capacitor/push-notifications": "^7.0.2",
    "@capacitor/status-bar": "7.0.1",
    "@ionic/react": "^8.5.0",
    "@ionic/react-router": "^8.5.0",
    "@supabase/supabase-js": "^2.53.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "ionicons": "^7.4.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.6.1",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@capacitor/cli": "7.4.2",
    "@playwright/test": "^1.54.2",
    "@size-limit/preset-app": "^11.2.0",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^24.1.0",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.1",
    "eslint": "^9.20.1",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^22.1.0",
    "msw": "^2.10.4",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "size-limit": "^11.2.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.4.0",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.24.0",
    "vite": "5.2.14",
    "vitest": "^0.34.6"
  },
  "overrides": {
    "rollup": "4.44.0"
  },
  "size-limit": [
    {
      "name": "Main Bundle",
      "path": "dist/assets/index-*.js",
      "limit": "250 kB",
      "gzip": true,
      "running": false
    },
    {
      "name": "Total JS",
      "path": "dist/assets/*.js",
      "limit": "800 kB",
      "gzip": true,
      "running": false
    }
  ],
  "description": "An Ionic project"
}
